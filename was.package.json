  "files" : [ "proto/wtools/atop/transpile", "proto/wtools/Tools.s", "out" ],
  "scripts" :
  {
    "test" : "wtest .run 'proto/**' rapidity:-3",
    "docgen" : "wdocgen proto",
    "trs" : "node proto/wtools/atop/transpile/entry/Exec"
  },
  "repository" :
  {
    "type" : "git",
    "url" : "https://github.com/Wandalen/wTranspile.git"
