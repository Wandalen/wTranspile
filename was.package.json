{
  "name": "wtranspile",
  "version": "0.3.277",
  "enabled": 1,
  "description": "Aggregator of strategies to transpile JS code. It provides unified programmatic and CL interfaces to transpile/optimize/minimize/beautify code by one or several transpilers in series. More strategies could be added as plugins. Default options of transpilation can be written into a config file to avoid retyping. Use the module to utilize the power of open source transpilation tools in single package.",
  "author": "Kostiantyn Wandalen <wanalen@01.school>",
  "license": "MIT",
  "main": "proto/node_modules/wtranspile",
  "bin": {
    "trs": "proto/wtools/atop/transpile/entry/Exec"
  },
  "files": [
    "proto/wtools/atop/transpile",
    "out",
    "proto/node_modules",
    "proto/node_modules/Tools",
    "proto/node_modules/wtranspile"
  ],
  "scripts": {
    "test": "wtest .run 'proto/**' rapidity:-3",
    "docgen": "wdocgen proto",
    "trs": "node proto/wtools/atop/transpile/entry/Exec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wandalen/wTranspile.git"
  },
  "bugs": {
    "url": "https://github.com/Wandalen/wTranspile/issues"
  },
  "dependencies": {
    "wverbal": "",
    "wTools": "",
    "wstatestorage": "",
    "wstatesession": "",
    "wstarter": "",
    "wpathbasic": "",
    "wFiles": "",
    "wCopyable": "",
    "wcommandsconfig": "",
    "wcommandsaggregator": "",
    "uglify-js": "=3.13.10",
    "uglify-es": "=3.3.9",
    "google-closure-compiler-js": "=20200517.0.0",
    "babylon": "=6.18.0",
    "babel-preset-node6-without-strict": "=0.0.1",
    "babel-preset-es2015-without-strict": "=0.0.4",
    "babel-preset-es2015": "=6.24.1",
    "@babel/preset-stage-3": "=7.8.3",
    "@babel/preset-stage-2": "=7.8.3",
    "@babel/preset-stage-1": "=7.8.3",
    "@babel/preset-stage-0": "=7.8.3",
    "@babel/preset-env": "=7.11.5",
    "@babel/polyfill": "=7.11.5",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/core": "=7.11.6"
  },
  "devDependencies": {
    "wTesting": "",
    "eslint": "7.1.0"
  },
  "keywords": [
    "wTools",
    "Transpilation",
    "transpilation strategies",
    "uglify",
    "uglify-es",
    "uglify-js",
    "beautify",
    "minify",
    "minimize",
    "optimize",
    "minifier",
    "javascript",
    "ecmascript"
  ]
}
